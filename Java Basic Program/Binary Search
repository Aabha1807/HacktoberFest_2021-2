import java.util.*;

public class binarySearch{
  public static void search(int[] array, int x){
    int low = 0;
    int high = arr.length-1;
    while(start<=end){
      int mid = (high+low)/2;
      if(arr[mid]==x){
        return mid;
      }
      else if(arr[mid]>x){
        high = mid-1;
      }
      else{
        low = mid+1;
      }
    }
    return -1;
  }
}
